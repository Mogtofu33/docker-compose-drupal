{
    "name": "mogtofu33/docker-minimal-dashboard",
    "description": "A minimal Php docker dashboard for Docker Compose Drupal.",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "docker-php/docker-php": "^1.24"
    },
    "require-dev": {
        "symfony/var-dumper": "^3.3"
    },
    "autoload": {
        "psr-4": { "Dashboard\\": "src/" }
    },
    "extra": {
      "phar-builder": {
          "compression": "GZip",
          "name": "dashboard.phar",
          "output-dir": "../build/",
          "entry-point": "index.php",
          "skip-shebang": true,
          "include": ["js"],
          "include-dev": false,
          "events": {
              "build.after": [
                  "chmod +x ../dashboard.phar"
              ]
          }
      }
    }
}
